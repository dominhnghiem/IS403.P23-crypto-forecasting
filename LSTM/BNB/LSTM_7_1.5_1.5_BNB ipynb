 drive\n","drive.mount('/content/drive')\n","\n","train_path = '/content/drive/MyDrive/Colab Notebooks/7_1.5_1.5/bnb_train.csv'\n","val_path   = '/content/drive/MyDrive/Colab Notebooks/7_1.5_1.5/bnb_val.csv'\n","test_path  = '/content/drive/MyDrive/Colab Notebooks/7_1.5_1.5/bnb_test.csv'\n","\n","train_df = pd.read_csv(train_path, parse_dates=['Date'], index_col='Date')\n","val_df   = pd.read_csv(val_path,   parse_dates=['Date'], index_col='Date')\n","test_df  = pd.read_csv(test_path,  parse_dates=['Date'], index_col='Date')\n","\n","train_df.head()"]},{"cell_type":"markdown","id":"f53fea01","metadata":{"id":"f53fea01"},"source":["## 2. Tiền xử lý và scale cột `Price`"]},{"cell_type":"code","execution_count":null,"id":"2cf41a59","metadata":{"id":"2cf41a59"},"outputs":[],"source":["scaler = MinMaxScaler()\n","train_prices = train_df[['Price']].values\n","scaled_train = scaler.fit_transform(train_prices)\n","\n","val_prices  = val_df[['Price']].values\n","scaled_val   = scaler.transform(val_prices)\n","\n","test_prices = test_df[['Price']].values\n","scaled_test  = scaler.transform(test_prices)"]},{"cell_type":"markdown","id":"b629d8c6","metadata":{"id":"b629d8c6"},"source":["## 3. Tạo sequence cho LSTM"]},{"cell_type":"code","execution_count":null,"id":"fd13583f","metadata":{"id":"fd13583f"},"outputs":[],"source":["def create_sequences(data, time_steps=100):\n","    X, y = [], []\n","    for i in range(len(data) - time_steps):\n","        X.append(data[i: i + time_steps])\n","        y.append(data[i + time_steps, 0])\n","    return np.array(X), np.array(y)\n","\n","time_steps = 100\n","X_train, y_train = create_sequences(scaled_train, time_steps)\n","X_val,   y_val   = create_sequences(scaled_val,   time_steps)\n","X_test,  y_test  = create_sequences(scaled_test,  time_steps)\n","\n","# reshape for LSTM: (samples, time_steps, features)\n","X_train = X_train.reshape((X_train.shape[0], time_steps, 1))\n","X_val   = X_val.reshape((X_val.shape[0], time_steps, 1))\n","X_test  = X_test.reshape((X_test.shape[0], time_steps, 1))"]},{"cell_type":"markdown","id":"78a79f73","metadata":{"id":"78a79f73"},"source":["## 4. Xây dựng và huấn luyện mô hình LSTM"]},{"cell_type":"code","execution_count":null,"id":"fa208ae5","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"fa208ae5","executionInfo":{"status":"ok","timestamp":1748164373106,"user_tz":-420,"elapsed":736250,"user":{"displayName":"Phúc Từ Thị Hồng","userId":"17528141503518784554"}},"outputId":"b6773dff-f442-4042-c75e-f8b73fb53bc8"},"outputs":[{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.11/dist-packages/keras/src/layers/rnn/rnn.py:200: UserWarning: Do not pass an `input_shape`/`input_dim` argument to a layer. When using Sequential models, prefer using an `Input(shape)` object as the first layer in the model instead.\n","  super().__init__(**kwargs)\n"]},{"output_type":"display_data","data":{"text/plain":["\u001b[1mModel: \"sequential\"\u001b[0m\n"],"text/html":["<pre style=\"white-space:pre;overflow-x:auto;line-height:normal;font-family:Menlo,'DejaVu Sans Mono',consolas,'Courier New',monospace\"><span style=\"font-weight: bold\">Model: \"sequential\"</span>\n","</pre>\n"]},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┓\n","┃\u001b[1m \u001b[0m\u001b[1mLayer (type)                   \u001b[0m\u001b[1m \u001b[0m┃\u001b[1m \u001b[0m\u001b[1mOutput Shape          \u001b[0m\u001b[1m \u001b[0m┃\u001b[1m \u001b[0m\u001b[1m      Param #\u001b[0m\u001b[1m \u001b[0m┃\n","┡━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━┩\n","│ lstm (\u001b[38;5;33mLSTM\u001b[0m)                     │ (\u001b[38;5;45mNone\u001b[0m, \u001b[38;5;34m100\u001b[0m, \u001b[38;5;34m50\u001b[0m)        │        \u001b[38;5;34m10,400\u001b[0m │\n","├─────────────────────────────────┼────────────────────────┼───────────────┤\n","│ lstm_1 (\u001b[